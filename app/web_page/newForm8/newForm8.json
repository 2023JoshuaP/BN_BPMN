    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.solicitudAperturaCuenta ){",
        "\treturn {",
        "\t\t//map solicitudAperturaCuenta variable to expected task contract input",
        "\t\tsolicitudAperturaCuentaInput: {",
